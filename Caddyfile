easternestate.global {
    @api path /api/v1* 
    handle @api {
        reverse_proxy backend:3001
    }

    # Serve uploaded files directly from shared volume
    handle /uploads/* {
        root * /srv
        file_server
    }

    handle {
        reverse_proxy frontend:3000
    }
}

www.easternestate.global {
    redir https://easternestate.global{uri}
}
